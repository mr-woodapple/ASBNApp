{
  "routes": [
    {
      // Routes all routes prefixed with /api to another URL, effectively using the Static Web App as a reverse proxy
      "route": "/api/*",
      "rewrite": "https://jsonplaceholder.typicode.com/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*" ]
  }
}